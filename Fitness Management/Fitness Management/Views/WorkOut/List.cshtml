<!-- Views/WorkOut/List.cshtml -->
@model IEnumerable<Fitness_Management.Models.WorkOutDto>
@{
    ViewBag.Title = "List WorkOuts";
}

<h2>List of WorkOuts</h2>

<table class="table">
    <tr>
        <th>WorkOut Name</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>
                <a href="@Url.Action("Details", "WorkOut", new { id = item.WorkOutId })">Details</a>
            </td>
        </tr>
    }
</table>
